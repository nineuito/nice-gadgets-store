{
  "extends": "@mate-academy/students-ts-config",
  "include": [
    "src",
    "src/types"
  ],
  "compilerOptions": {
    "sourceMap": false,
    "types": ["node", "cypress"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "styles/*": ["src/styles/*"],
      "data/*": ["src/data/*"]
    }
  }
}
